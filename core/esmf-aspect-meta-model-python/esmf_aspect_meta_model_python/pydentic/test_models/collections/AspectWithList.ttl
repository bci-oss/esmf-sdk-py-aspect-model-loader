

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:org.eclipse.esmf:1.0.0#> .
@prefix samm-u: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .

:AspectWithList a samm:Aspect ;
   samm:preferredName "Test aspectwith list elements"@en ;
   samm:properties ( :propertyWithEntityList :propertyWithScalarList ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:propertyWithEntityList a samm:Property ;
   samm:preferredName "Property with list of entity elements"@en ;
   samm:description "Property with list of entity elements"@en ;
   samm:characteristic :EntityList .

:propertyWithScalarList a samm:Property ;
   samm:preferredName "Property with list of scalar elements"@en ;
   samm:description "Property with list of scalar  elements"@en ;
   samm:characteristic :ScalarList .

:EntityList a samm:Characteristic ;
   samm:preferredName "List with entity values"@en ;
   samm:description "List with entity values"@en ;
   samm:dataType :TestEntity .

:ScalarList a samm-c:List ;
   samm:preferredName "List with scalar values"@en ;
   samm:description "List with scalar values"@en .

:TestEntity a samm:Entity ;
   samm:preferredName "Test element"@en ;
   samm:description "Element for the test purpose"@en ;
   samm:properties ( :elementName ) .

:elementName a samm:Property ;
   samm:preferredName "Name of the element"@en ;
   samm:description "Name of the user defined element"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "some name" .
