@prefix : <urn:samm:org.eclipse.esmf.samm.test:1.0.0#> .
@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.0.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.0.0#> .

:AspectWithLengthConstraint a samm:Aspect ;
   samm:properties ( :testProperty ) ;
   samm:operations ( ) .

:testProperty a samm:Property ;
   samm:exampleValue "Test123" ;
   samm:characteristic :TestLengthConstraint .

:TestLengthConstraint a samm-c:Trait ;
   samm-c:constraint [
      a samm-c:LengthConstraint ;
      samm-c:minValue "5"^^xsd:nonNegativeInteger ;
      samm-c:maxValue "10"^^xsd:nonNegativeInteger ;
   ] ;
   samm-c:baseCharacteristic samm-c:Text .
